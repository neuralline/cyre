{
  "name": "cyre",
  "version": "4.4.0",
  "author": "Cyre Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neuralline/cyre.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "devDependencies": {
    "@jest/globals": "30.0.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/bun": "latest",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-dts": "^6.2.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bugs": {
    "url": "https://github.com/neuralline/cyre/issues"
  },
  "description": "Neural Line - Reactive event manager with zero dependencies",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/neuralline/cyre#readme",
  "keywords": [
    "reactive",
    "events",
    "state-management",
    "functional",
    "typescript",
    "zero-dependency",
    "performance"
  ],
  "license": "MIT",
  "scripts": {
    "build": "bun run clean && bun run build:types && bun run build:esm",
    "build:vite": "vite build",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:esm": "bun build ./src/index.ts --outfile ./dist/index.js --format esm",
    "build:cjs": "bun build ./src/index.ts --outfile ./dist/index.cjs --format cjs",
    "clean": "rm -rf dist",
    "test": "bun test",
    "vitest": "vitest run",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "bun run typecheck",
    "benchmark": "bun run example/industry-standard-tests.ts",
    "dev": "tsx scripts/cyre-server.ts"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
