{
  "name": "cyre",
  "version": "3.1.5",
  "description": "Reactive and dynamic networking system with quantum-timed event orchestration, intelligent load balancing, and advanced state management for JavaScript applications",
  "input": "src/index.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.js",
  "browser": "dist/umd/cyre.js",
  "unpkg": "dist/umd/cyre.min.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf dist && vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "prepublishOnly": "npm run build"
  },
  "author": "Darik.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neuralline/cyre.git"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "rimraf": "^6.0.1",
    "terser": "^5.39.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "keywords": [
    "pub/sub",
    "publish/subscribe",
    "reactive",
    "subscribe",
    "event manager",
    "event",
    "action-on-call",
    "reducers",
    "debounce",
    "redux alternative"
  ],
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/umd/cyre.js",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
