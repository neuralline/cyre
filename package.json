  },
  "author": "Darik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neuralline/cyre.git"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.15.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vitest": "^3.1.2"
  },
  "keywords": [
    "pub/sub",
    "publish/subscribe",
    "reactive",
    "subscribe",
